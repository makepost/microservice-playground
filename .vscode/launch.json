{
  // Use IntelliSense to learn about possible Node.js debug attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Arbitration Service",
      "program": "${workspaceRoot}/bin/arbitrationService.js",
      "protocol": "inspector" // ES6 proxy support.
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Arbitration Admin",
      "program": "${workspaceRoot}/bin/arbitrationAdmin.js",
      "protocol": "inspector" // ES6 proxy support.
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Conversion Client",
      "program": "${workspaceRoot}/bin/conversionClient.js",
      "protocol": "inspector" // ES6 proxy support.
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Conversion Service",
      "program": "${workspaceRoot}/bin/conversionService.js",
      "protocol": "inspector" // ES6 proxy support.
    },
    {
      "type": "node",
      "request": "launch",
      "name": "Mocha Tests",
      "program": "${workspaceRoot}/node_modules/mocha/bin/_mocha",
      "args": [
        "--bail",
        "-u",
        "tdd",
        "--timeout",
        "999999",
        "--colors",
        "${workspaceRoot}/out/**/*.test.js"
      ],
      "internalConsoleOptions": "openOnSessionStart"
    }
  ]
}
